      "source": [
        "year=int(input(\"請輸入一個年份:\"))\n",
        "if year<=1582:\n",
        "  result=year%4==0\n",
        "else:\n",
        "  result=(year%4==0 and year%100!=0)or(year%400==0)\n",
        "if result:\n",
        "  print(year,\"是閏年\")\n",
        "else:\n",
        "  print(year,\"是平年\")\n",
        "\n"
      ]
